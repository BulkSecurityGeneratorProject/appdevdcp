<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Questionario.
    -->
    <changeSet id="20181111155651-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="questionarios_id"
                                 baseTableName="questionario_grupo"
                                 constraintName="fk_questionario_grupo_questionarios_id"
