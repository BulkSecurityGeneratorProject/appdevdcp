                                 baseTableName="questionario_grupo"
                                 constraintName="fk_questionario_grupo_grupos_id"
                                 referencedColumnNames="id"
                                 referencedTableName="grupo_itens"/>
        
    </changeSet>
</databaseChangeLog>
